<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.proccon">
    <signal name="fork">
      <arg name="parent_pid" type="i" direction="out"/>
      <arg name="parent_tgid" type="i" direction="out"/>
      <arg name="child_pid" type="i" direction="out"/>
      <arg name="child_tgid" type="i" direction="out"/>
    </signal>
    <signal name="exec">
      <arg name="process_pid" type="i" direction="out"/>
      <arg name="process_tgid" type="i" direction="out"/>
    </signal>
    <signal name="uid">
      <arg name="process_pid" type="i" direction="out"/>
      <arg name="process_tgid" type="i" direction="out"/>
      <arg name="ruid" type="i" direction="out"/>
      <arg name="euid" type="i" direction="out"/>
    </signal>
    <signal name="gid">
      <arg name="process_pid" type="i" direction="out"/>
      <arg name="process_tgid" type="i" direction="out"/>
      <arg name="rgid" type="i" direction="out"/>
      <arg name="egid" type="i" direction="out"/>
    </signal>
    <signal name="exit">
      <arg name="process_pid" type="i" direction="out"/>
      <arg name="process_tgid" type="i" direction="out"/>
      <arg name="exit_code" type="i" direction="out"/>
    </signal>
  </interface>
</node>
